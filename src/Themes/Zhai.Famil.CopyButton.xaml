<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Zhai.Famil.Controls">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/Zhai.Famil;component/Themes/Zhai.Famil.IconButton.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style TargetType="{x:Type local:CopyButton}" x:Key="Zhai.Famil.CopyButton" BasedOn="{StaticResource Zhai.Famil.IconButton}">
        <Setter Property="Icon" Value="VuesaxOutlineDocumentCopy"/>
    </Style>

</ResourceDictionary>